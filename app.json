{
  "expo": {
    "name": "G F@ker",
    "slug": "gFaker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/hack.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/sigma.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/hack.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.reharish.gFaker"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(name) to use your location."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "f48586d1-a2ab-4fe5-bd37-4d1d703a8556"
      }
    }
  }
}
